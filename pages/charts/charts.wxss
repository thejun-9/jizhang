/**index.wxss**/
.echart_panel{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
ec-canvas {
  width: 100%;
  height: 100%;
}
.picker{
  background-color: cornflowerblue;
  text-align: center;
  border-radius:10px;
  border:1px solid #292626;
  font-size: medium;
}
